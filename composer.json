{
    "name": "wyn/larvel-google-secret-manager",
    "description": "Manage environment secrets using Google Secrets Manager (GSM).",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Vendor\\GoogleSecretManager\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/wahyuyn/larvel-google-secret-manager"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "google/cloud-secret-manager": "^1.15"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "wahyu",
            "email": "wahyu.yn15@gmail.com"
        }
    ],
    "minimum-stability": "alpha",
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\GoogleSecretManager\\GoogleSecretManagerServiceProvider"
            ]
        }
    }
}
